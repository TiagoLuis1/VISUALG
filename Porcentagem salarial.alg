algoritmo "semnome"
var
nome,totalSalario,porcentagem:caractere
salario:real
totalVendas:real
inicio
escreva("nome do trabalhador: ")
leia(nome)
escreva("salario do trabalhador: ")
leia(salario)
escreva("total de vendas: ")
leia(totalVendas)

escreva("total de: ")
escreva(salario+totalVendas*15)
escreva(totalSalario)

fimalgoritmo
